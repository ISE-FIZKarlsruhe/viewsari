     -->

    


    <!-- http://prismstandard.org/namespaces/basic/2.0/endingPage -->

    <owl:DatatypeProperty rdf:about="http://prismstandard.org/namespaces/basic/2.0/endingPage">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#Endeavour"/>
        <rdfs:comment xml:lang="en">Identifies the last page of an entity such as a journal article.</rdfs:comment>
        <rdfs:label xml:lang="en">has ending page</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://prismstandard.org/namespaces/basic/2.0/pageRange -->

    <owl:DatatypeProperty rdf:about="http://prismstandard.org/namespaces/basic/2.0/pageRange">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#Endeavour"/>
        <rdfs:comment xml:lang="en">Identifies the page range of an entity such as a journal article.</rdfs:comment>
        <rdfs:label xml:lang="en">has page range</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://prismstandard.org/namespaces/basic/2.0/publicationDate -->

    <owl:DatatypeProperty rdf:about="http://prismstandard.org/namespaces/basic/2.0/publicationDate">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/issued"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#Endeavour"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
                    <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#gYear"/>
                    <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#gYearMonth"/>
                </owl:unionOf>
            </rdfs:Datatype>
        </rdfs:range>
        <dc:description xml:lang="en">The date on which a document or entity is published.
The date can be expressed in three different formats:
- yyyy-mm-dd (i.e., xsd:date)
- yyyy-mm (i.e., xsd:gYearMonth)
- yyyy (i.e., xsd:gYear)</dc:description>
        <rdfs:comment xml:lang="en">The date on which a resource is published or disclosed.</rdfs:comment>
        <rdfs:label xml:lang="en">has publication date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://prismstandard.org/namespaces/basic/2.0/startingPage -->

    <owl:DatatypeProperty rdf:about="http://prismstandard.org/namespaces/basic/2.0/startingPage">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#Endeavour"/>
        <rdfs:comment xml:lang="en">Identifies the first page of an entity such as a journal article.</rdfs:comment>
        <rdfs:label xml:lang="en">has starting page</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://prismstandard.org/namespaces/basic/2.0/volume -->

    <owl:DatatypeProperty rdf:about="http://prismstandard.org/namespaces/basic/2.0/volume">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#Endeavour"/>
        <rdfs:comment xml:lang="en">An identifier for a particular volume of a resource, such as a journal or a multi-volume book.</rdfs:comment>
        <rdfs:label xml:lang="en">has volume identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/terms/identifier -->

    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/identifier"/>
    


    <!-- http://purl.org/dc/terms/issued -->

    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/issued"/>
    


    <!-- http://purl.org/dc/terms/title -->

    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/title">
        <rdfs:comment xml:lang="en">A name given to a resource.  Typically, the title will be the name by which the resource is formally known.</rdfs:comment>
        <rdfs:label xml:lang="en">has title</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasPublicationYear -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasPublicationYear">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/issued"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#gYear"/>
        <rdfs:comment xml:lang="en">The year in which a resource is published.</rdfs:comment>
        <rdfs:label xml:lang="en">has publication year</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasTranslatedTitle -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasTranslatedTitle">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/title"/>
        <rdfs:comment xml:lang="en">A version of the title of an entity translated into another language, which may be specified using the object property dcterms:language.</rdfs:comment>
        <rdfs:label xml:lang="en">has translated title</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasURL -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasURL">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#Endeavour"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment xml:lang="en">An identifier, in form of an HTTP Universal Resource Locator (URL), for a particular resource on the World Wide Web.</rdfs:comment>
        <rdfs:label xml:lang="en">has URL</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasVolumeCount -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasVolumeCount">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#Endeavour"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
        <rdfs:comment xml:lang="en">The count of the number of volumes a work includes.</rdfs:comment>
        <rdfs:label xml:lang="en">has volume count</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://viewsari.github.io/ontology#hasPageRange -->

    <owl:DatatypeProperty rdf:about="http://viewsari.github.io/ontology#hasPageRange">
        <rdfs:subPropertyOf rdf:resource="http://prismstandard.org/namespaces/basic/2.0/pageRange"/>
        <rdfs:domain rdf:resource="http://viewsari.github.io/ontology#Biography"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://viewsari.github.io/ontology#lengthInCharacters -->

    <owl:DatatypeProperty rdf:about="http://viewsari.github.io/ontology#lengthInCharacters">
        <rdfs:domain rdf:resource="http://purl.org/spar/doco/Paragraph"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/oa#bodyValue -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/oa#bodyValue">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The object of the predicate is a plain text string to be used as the content of the body of the Annotation.  The value MUST be an  xsd:string  and that data type MUST NOT be expressed in the serialization. Note that language MUST NOT be associated with the value either as a language tag, as that is only available for  rdf:langString .
  </rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/oa#"/>
        <rdfs:label>bodyValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/oa#end -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/oa#end">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
        <rdfs:comment>The end property is used to convey the 0-based index of the end position of a range of content.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/oa#"/>
        <rdfs:label>end</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/oa#start -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/oa#start">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
        <rdfs:comment>The start position in a 0-based index at which a range of content is selected from the data in the source resource.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/oa#"/>
        <rdfs:label>start</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/prov#endedAtTime -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/prov#endedAtTime">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Activity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">The time at which an activity ended. See also prov:startedAtTime.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/prov-o#"/>
        <rdfs:label>endedAtTime</rdfs:label>
        <prov:category>starting-point</prov:category>
        <prov:component>entities-activities</prov:component>
        <prov:editorialNote xml:lang="en">It is the intent that the property chain holds: (prov:qualifiedEnd o prov:atTime) rdfs:subPropertyOf prov:endedAtTime.</prov:editorialNote>
        <prov:qualifiedForm rdf:resource="http://www.w3.org/ns/prov#End"/>
        <prov:qualifiedForm rdf:resource="http://www.w3.org/ns/prov#atTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/prov#startedAtTime -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/prov#startedAtTime">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Activity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">The time at which an activity started. See also prov:endedAtTime.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/prov-o#"/>
        <rdfs:label>startedAtTime</rdfs:label>
        <prov:category>starting-point</prov:category>
        <prov:component>entities-activities</prov:component>
        <prov:editorialNote xml:lang="en">It is the intent that the property chain holds: (prov:qualifiedStart o prov:atTime) rdfs:subPropertyOf prov:startedAtTime.</prov:editorialNote>
        <prov:qualifiedForm rdf:resource="http://www.w3.org/ns/prov#Start"/>
        <prov:qualifiedForm rdf:resource="http://www.w3.org/ns/prov#atTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://xmlns.com/foaf/0.1/name -->

    <owl:DatatypeProperty rdf:about="http://xmlns.com/foaf/0.1/name">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>A name for some thing.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://xmlns.com/foaf/0.1/"/>
        <rdfs:label>name</rdfs:label>
        <ns:term_status>testing</ns:term_status>
    </owl:DatatypeProperty>
    


