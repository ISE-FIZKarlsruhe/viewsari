     -->

    


    <!-- http://purl.org/spar/deo/Biography -->

    <owl:Class rdf:about="http://purl.org/spar/deo/Biography">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/deo/DiscourseElement"/>
        <rdfs:comment xml:lang="en">Information describing a person and his or her life history and contributions.</rdfs:comment>
        <rdfs:label xml:lang="en">biography</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/deo/DiscourseElement -->

    <owl:Class rdf:about="http://purl.org/spar/deo/DiscourseElement">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="en">An element of a document that carries out a rhetorical function.</rdfs:comment>
        <rdfs:label xml:lang="en">discourse element</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/doco/Paragraph -->

    <owl:Class rdf:about="http://purl.org/spar/doco/Paragraph">
        <rdfs:comment xml:lang="en">A self-contained unit of discourse that deals with a particular point or idea. Paragraphs contains one or more sentences. The start of a paragraph is indicated by beginning on a new line, which may be indented or separated by a small vertical space by the preceding paragraph. </rdfs:comment>
        <rdfs:label xml:lang="en">paragraph</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/doco/TextChunk -->

    <owl:Class rdf:about="http://purl.org/spar/doco/TextChunk">
        <rdfs:subClassOf rdf:resource="http://www.essepuntato.it/2008/12/pattern#Inline"/>
        <rdfs:comment xml:lang="en">A piece of text defined by a start point and an end point.</rdfs:comment>
        <rdfs:label xml:lang="en">text chunk</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/fabio/AnalogManifestation -->

    <owl:Class rdf:about="http://purl.org/spar/fabio/AnalogManifestation">
        <owl:disjointWith rdf:resource="http://purl.org/spar/fabio/DigitalManifestation"/>
    </owl:Class>
    


    <!-- http://purl.org/spar/fabio/Book -->

    <owl:Class rdf:about="http://purl.org/spar/fabio/Book">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/fabio/Expression"/>
        <rdfs:comment xml:lang="en">A non-serial document that is complete in one volume or a designated finite number of volumes.  A book published by a publisher is usually  identified by an International Standard Book Number (ISBN), and may be manifested as a physical printed publication on paper bound in a hard or soft cover, or in electronic format as an &apos;e-book&apos;.</rdfs:comment>
        <rdfs:label xml:lang="en">book</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/fabio/BookSeries -->

    <owl:Class rdf:about="http://purl.org/spar/fabio/BookSeries">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/fabio/Series"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#part"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/spar/fabio/Book"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://purl.org/spar/fabio/BookSet"/>
        <rdfs:comment xml:lang="en">A sequence of books having certain characteristics in common that are formally identified together as a group - for instance, the books in the Law, Governance and Technology Series published by Springer.</rdfs:comment>
        <rdfs:label xml:lang="en">book series</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/fabio/BookSet -->

    <owl:Class rdf:about="http://purl.org/spar/fabio/BookSet"/>
    


    <!-- http://purl.org/spar/fabio/DigitalItem -->

    <owl:Class rdf:about="http://purl.org/spar/fabio/DigitalItem"/>
    


    <!-- http://purl.org/spar/fabio/DigitalManifestation -->

    <owl:Class rdf:about="http://purl.org/spar/fabio/DigitalManifestation">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/fabio/Manifestation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#exemplar"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/spar/fabio/DigitalItem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A manifestation that represents data in binary form, encoding the data as a series of 0s and 1s.</rdfs:comment>
        <rdfs:label xml:lang="en">digital manifestation</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/fabio/Expression -->

    <owl:Class rdf:about="http://purl.org/spar/fabio/Expression">
        <rdfs:subClassOf rdf:resource="http://purl.org/vocab/frbr/core#Expression"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#part"/>
                        <owl:allValuesFrom rdf:resource="http://purl.org/vocab/frbr/core#Expression"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#partOf"/>
                        <owl:allValuesFrom rdf:resource="http://purl.org/vocab/frbr/core#Expression"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#embodiment"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/spar/fabio/Manifestation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#realizationOf"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/spar/fabio/Work"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description xml:lang="en">A fabio:Expression can only have part or be part of another fabio:Expression.  Moreover, it can be a representation only of a fabio:Work, and  it can be embodied only in fabio:Manifestation(s).</dc:description>
        <rdfs:comment xml:lang="en">A subclass of FRBR expression, restricted to expressions of fabio:Works.  For your latest research paper, the preprint submitted to the publisher, and the final published version to which the publisher assigned a unique digital object identifier, are both expressions of the same work.  </rdfs:comment>
        <rdfs:label xml:lang="en">expression</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/fabio/ExpressionCollection -->

    <owl:Class rdf:about="http://purl.org/spar/fabio/ExpressionCollection">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/fabio/Expression"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#part"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/spar/fabio/Expression"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A collection of expressions, for example a periodical or a book series.</rdfs:comment>
        <rdfs:label xml:lang="en">expression collection</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/fabio/Manifestation -->

    <owl:Class rdf:about="http://purl.org/spar/fabio/Manifestation">
        <rdfs:subClassOf rdf:resource="http://purl.org/vocab/frbr/core#Manifestation"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#part"/>
                        <owl:allValuesFrom rdf:resource="http://purl.org/vocab/frbr/core#Manifestation"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#partOf"/>
                        <owl:allValuesFrom rdf:resource="http://purl.org/vocab/frbr/core#Manifestation"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#embodimentOf"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/spar/fabio/Expression"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description xml:lang="en">A fabio:Manifestation can only have part or be part of another fabio:Manifestation.  Moreover, it can be an embodiment only of a fabio:Expression and it can be exemplified only by fabio:Item(s).</dc:description>
        <rdfs:comment xml:lang="en">A subclass of FRBR manifestation, restricted to manifestations of fabio:Expressions. fabio:Manifestation specifically applies to electronic (digital) as well as to physical manifestations of expressions.  

Examples of different manifestations of a single &apos;version of record&apos; expression of a scholarly work include an article in a print journal or the on-line version of that article as a web page.</rdfs:comment>
        <rdfs:label xml:lang="en">manifestation</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/fabio/Page -->

    <owl:Class rdf:about="http://purl.org/spar/fabio/Page"/>
    


    <!-- http://purl.org/spar/fabio/Series -->

    <owl:Class rdf:about="http://purl.org/spar/fabio/Series">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/fabio/ExpressionCollection"/>
        <rdfs:comment xml:lang="en">A sequence of expressions having certain characteristics in common that are formally identified together as a group.</rdfs:comment>
        <rdfs:label xml:lang="en">series</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/fabio/WebManifestation -->

    <owl:Class rdf:about="http://purl.org/spar/fabio/WebManifestation">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/fabio/DigitalManifestation"/>
        <rdfs:comment xml:lang="en">A digital manifestation on the Web, such as a wiki, a web site, a web page or a blog.</rdfs:comment>
        <rdfs:label xml:lang="en">web manifestation</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/fabio/WebPage -->

    <owl:Class rdf:about="http://purl.org/spar/fabio/WebPage">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/fabio/Page"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/fabio/WebManifestation"/>
        <rdfs:comment xml:lang="en">A Web manifestation usually identified by a Uniform Resource Identifier (URI), and made accessible to a user by means of the Hypertext Transport Protocol (HTTP) in a Web browser window. Several interlinked web pages hosted together on a Web server and accessed through a single domain name or IP address constitute a web site.</rdfs:comment>
        <rdfs:label xml:lang="en">web page</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/fabio/Work -->

    <owl:Class rdf:about="http://purl.org/spar/fabio/Work">
        <rdfs:subClassOf rdf:resource="http://purl.org/vocab/frbr/core#Work"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#part"/>
                        <owl:allValuesFrom rdf:resource="http://purl.org/vocab/frbr/core#Work"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#partOf"/>
                        <owl:allValuesFrom rdf:resource="http://purl.org/vocab/frbr/core#Work"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#realization"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/spar/fabio/Expression"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description xml:lang="en">A fabio:Work can only have part or be part of another fabio:Work. Moreover, it can be realized only by fabio:Expression(s).</dc:description>
        <rdfs:comment xml:lang="en">A subclass of FRBR work, restricted to works that are published or potentially publishable, and that contain or are referred to by bibliographic references, or entities used to define bibliographic references. FaBiO works, and their expressions and manifestations, are primarily textual publications such as books, magazines, newspapers and journals, and items of their content.  However, they also include datasets, computer algorithms, experimental protocols, formal specifications and vocabularies, legal records, governmental papers, technical and commercial reports and similar publications, and also bibliographies, reference lists, library catalogues and similar collections. For this reason, fabio:Work is not an equivalent class to frbr:ScholarlyWork.  An example of a fabio:Work is your latest research paper.</rdfs:comment>
        <rdfs:label xml:lang="en">work</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/vocab/frbr/core#Endeavour -->

    <owl:Class rdf:about="http://purl.org/vocab/frbr/core#Endeavour"/>
    


    <!-- http://purl.org/vocab/frbr/core#Expression -->

    <owl:Class rdf:about="http://purl.org/vocab/frbr/core#Expression"/>
    


    <!-- http://purl.org/vocab/frbr/core#Manifestation -->

    <owl:Class rdf:about="http://purl.org/vocab/frbr/core#Manifestation"/>
    


    <!-- http://purl.org/vocab/frbr/core#Work -->

    <owl:Class rdf:about="http://purl.org/vocab/frbr/core#Work"/>
    


    <!-- http://viewsari.github.io/ontology#Artwork -->

    <owl:Class rdf:about="http://viewsari.github.io/ontology#Artwork">
        <rdfs:subClassOf rdf:resource="http://viewsari.github.io/ontology#ContentResource"/>
        <rdfs:comment xml:lang="en">The Artwork class represents creative works of art, encompassing various mediums such as paintings, sculptures, and digital creations. It stores metadata such as title, artist, creation date, and style.</rdfs:comment>
    </owl:Class>
    


    <!-- http://viewsari.github.io/ontology#Biography -->

    <owl:Class rdf:about="http://viewsari.github.io/ontology#Biography">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/deo/Biography"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/fabio/DigitalManifestation"/>
        <rdfs:comment xml:lang="en">The Biography class encapsulates chapters within Vasari&apos;s seminal work that focus on the lives and works of Renaissance artists. It is about a person and has a title, a page range, a starting and end page.</rdfs:comment>
    </owl:Class>
    


    <!-- http://viewsari.github.io/ontology#ContentResource -->

    <owl:Class rdf:about="http://viewsari.github.io/ontology#ContentResource">
        <rdfs:comment xml:lang="en">The ContentResource class summarizes classes representing various types of extracted content and defines super-properties like labels and establishes the connection to the original sources from where said content was extracted.</rdfs:comment>
    </owl:Class>
    


    <!-- http://viewsari.github.io/ontology#Cooccurrence -->

    <owl:Class rdf:about="http://viewsari.github.io/ontology#Cooccurrence">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <rdfs:comment xml:lang="en">The Cooccurrence class represents a connection between a minimum of two content resources, such as persons (artists), artworks, or other entities. It encapsulates information about the provenance, context, frequency (score), and scope of the co-occurrence.</rdfs:comment>
    </owl:Class>
    


    <!-- http://viewsari.github.io/ontology#CooccurrenceScope -->

    <owl:Class rdf:about="http://viewsari.github.io/ontology#CooccurrenceScope">
        <rdfs:comment xml:lang="en">The Cooccurrence scope class specifies from which perspective on the work the cooccurrences have been extracted. Were entire volumes considered? Were they extracted within single biographies?</rdfs:comment>
    </owl:Class>
    


    <!-- http://viewsari.github.io/ontology#DiceCalculation -->

    <owl:Class rdf:about="http://viewsari.github.io/ontology#DiceCalculation">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Activity"/>
        <rdfs:comment xml:lang="en">The DiceCalculation is a specific type of activity performed to extract co-occurrences, based on the dice coefficient.</rdfs:comment>
    </owl:Class>
    


    <!-- http://viewsari.github.io/ontology#Edition -->

    <owl:Class rdf:about="http://viewsari.github.io/ontology#Edition">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/fabio/BookSeries"/>
        <rdfs:comment xml:lang="en">The Edition class in the realm of Viewsari represents a specific version or iteration of a literary work. Separate editions are characterized by differences in content and the publication year.
An edition is composed of several subsequent volumes.</rdfs:comment>
    </owl:Class>
    


    <!-- http://viewsari.github.io/ontology#HistoricalEvent -->

    <owl:Class rdf:about="http://viewsari.github.io/ontology#HistoricalEvent">
        <rdfs:subClassOf rdf:resource="http://viewsari.github.io/ontology#ContentResource"/>
