     -->

    


    <!-- http://purl.org/dc/terms/hasPart -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/hasPart"/>
    


    <!-- http://purl.org/dc/terms/isPartOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/isPartOf"/>
    


    <!-- http://purl.org/spar/fabio/hasManifestation -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/fabio/hasManifestation">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/vocab/frbr/core#relatedEndeavour"/>
        <owl:inverseOf rdf:resource="http://purl.org/spar/fabio/isManifestationOf"/>
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#Work"/>
        <rdfs:range rdf:resource="http://purl.org/vocab/frbr/core#Manifestation"/>
        <rdfs:comment xml:lang="en">A property linking a particular work to its manifestations.  This property is additional to the relationships between FRBR endeavours present in the classical FRBR data model.</rdfs:comment>
        <rdfs:label xml:lang="en">has manifestation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/fabio/isManifestationOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/fabio/isManifestationOf"/>
    


    <!-- http://purl.org/vocab/frbr/core#embodiment -->

    <owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#embodiment">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/vocab/frbr/core#relatedEndeavour"/>
        <owl:inverseOf rdf:resource="http://purl.org/vocab/frbr/core#embodimentOf"/>
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#Expression"/>
        <rdfs:range rdf:resource="http://purl.org/vocab/frbr/core#Manifestation"/>
        <rdfs:comment xml:lang="en">An expression embodied in a manifestation.</rdfs:comment>
        <rdfs:comment xml:lang="en">As defined by FRBR (http://www.ifla.org/VII/s13/frbr/frbr1.htm; http://purl.org/vocab/frbr/core#embodiment), a property representing a manifestation that embodies an expression.</rdfs:comment>
        <rdfs:label xml:lang="en">has embodiment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/vocab/frbr/core#embodimentOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#embodimentOf"/>
    


    <!-- http://purl.org/vocab/frbr/core#exemplar -->

    <owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#exemplar"/>
    


    <!-- http://purl.org/vocab/frbr/core#part -->

    <owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#part">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/vocab/frbr/core#relatedEndeavour"/>
        <owl:inverseOf rdf:resource="http://purl.org/vocab/frbr/core#partOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment xml:lang="en">A part of an endeavour.</rdfs:comment>
        <rdfs:comment xml:lang="en">Meaning = has part.  As defined by FRBR (http://www.ifla.org/VII/s13/frbr/frbr1.htm; http://purl.org/vocab/frbr/core#part),  a property indicating that one entity includes another entity.</rdfs:comment>
        <rdfs:label xml:lang="en">has part</rdfs:label>
        <skos1:note xml:lang="en">Unlike the FRBR version in RDF http://vocab.org/frbr/core.html the present version defines partonomy relationships transitive.</skos1:note>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/vocab/frbr/core#partOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#partOf"/>
    


    <!-- http://purl.org/vocab/frbr/core#realization -->

    <owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#realization">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/vocab/frbr/core#relatedEndeavour"/>
        <owl:inverseOf rdf:resource="http://purl.org/vocab/frbr/core#realizationOf"/>
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#Work"/>
        <rdfs:range rdf:resource="http://purl.org/vocab/frbr/core#Expression"/>
        <rdfs:comment xml:lang="en">A work realized through an expression.</rdfs:comment>
        <rdfs:comment xml:lang="en">As defined by FRBR (http://www.ifla.org/VII/s13/frbr/frbr1.htm; http://purl.org/vocab/frbr/core#realization), a property representing an expression that is an intellectual or artistic realization of a work.</rdfs:comment>
        <rdfs:label xml:lang="en">has realization</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/vocab/frbr/core#realizationOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#realizationOf"/>
    


    <!-- http://purl.org/vocab/frbr/core#relatedEndeavour -->

    <owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#relatedEndeavour"/>
    


    <!-- http://viewsari.github.io/ontology#basedOn -->

    <owl:ObjectProperty rdf:about="http://viewsari.github.io/ontology#basedOn">
        <rdfs:domain rdf:resource="http://viewsari.github.io/ontology#Translation"/>
        <rdfs:range rdf:resource="http://viewsari.github.io/ontology#Edition"/>
    </owl:ObjectProperty>
    


    <!-- http://viewsari.github.io/ontology#concernsResource -->

    <owl:ObjectProperty rdf:about="http://viewsari.github.io/ontology#concernsResource">
        <rdfs:domain rdf:resource="http://viewsari.github.io/ontology#ResourceInText"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://viewsari.github.io/ontology#concernsResource"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://viewsari.github.io/ontology#ContentResource"/>
            </owl:Restriction>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://viewsari.github.io/ontology#hasOccupation -->

    <owl:ObjectProperty rdf:about="http://viewsari.github.io/ontology#hasOccupation">
        <rdfs:domain rdf:resource="http://viewsari.github.io/ontology#Person"/>
        <rdfs:range rdf:resource="http://viewsari.github.io/ontology#Occupation"/>
    </owl:ObjectProperty>
    


    <!-- http://viewsari.github.io/ontology#hasScope -->

    <owl:ObjectProperty rdf:about="http://viewsari.github.io/ontology#hasScope">
        <rdfs:domain rdf:resource="http://viewsari.github.io/ontology#Cooccurrence"/>
        <rdfs:range rdf:resource="http://viewsari.github.io/ontology#CooccurrenceScope"/>
    </owl:ObjectProperty>
    


    <!-- http://viewsari.github.io/ontology#inParagraph -->

    <owl:ObjectProperty rdf:about="http://viewsari.github.io/ontology#inParagraph">
        <rdfs:domain rdf:resource="http://viewsari.github.io/ontology#Cooccurrence"/>
        <rdfs:range rdf:resource="http://purl.org/spar/doco/Paragraph"/>
    </owl:ObjectProperty>
    


    <!-- http://viewsari.github.io/ontology#involvedResource -->

    <owl:ObjectProperty rdf:about="http://viewsari.github.io/ontology#involvedResource">
        <rdfs:domain rdf:resource="http://viewsari.github.io/ontology#Cooccurrence"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://viewsari.github.io/ontology#involvedResource"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://viewsari.github.io/ontology#ContentResource"/>
            </owl:Restriction>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://viewsari.github.io/ontology#isAbout -->

    <owl:ObjectProperty rdf:about="http://viewsari.github.io/ontology#isAbout">
        <rdfs:domain rdf:resource="http://viewsari.github.io/ontology#Biography"/>
        <rdfs:range rdf:resource="http://viewsari.github.io/ontology#Person"/>
    </owl:ObjectProperty>
    


    <!-- http://viewsari.github.io/ontology#metionedIn -->

    <owl:ObjectProperty rdf:about="http://viewsari.github.io/ontology#metionedIn">
        <rdfs:domain rdf:resource="http://viewsari.github.io/ontology#ContentResource"/>
        <rdfs:range rdf:resource="http://purl.org/spar/doco/Paragraph"/>
    </owl:ObjectProperty>
    


    <!-- http://viewsari.github.io/ontology#provider -->

    <owl:ObjectProperty rdf:about="http://viewsari.github.io/ontology#provider">
        <rdfs:domain rdf:resource="http://purl.org/spar/fabio/DigitalManifestation"/>
        <rdfs:range rdf:resource="http://viewsari.github.io/ontology#Provider"/>
    </owl:ObjectProperty>
    


    <!-- http://www.essepuntato.it/2008/12/pattern#contains -->

    <owl:ObjectProperty rdf:about="http://www.essepuntato.it/2008/12/pattern#contains">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/hasPart"/>
        <owl:inverseOf rdf:resource="http://www.essepuntato.it/2008/12/pattern#isContainedBy"/>
        <rdfs:domain rdf:resource="http://www.essepuntato.it/2008/12/pattern#Structured"/>
        <rdfs:comment xml:lang="en">A structured element contains another generic element.</rdfs:comment>
        <rdfs:label xml:lang="en">contains</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.essepuntato.it/2008/12/pattern#isContainedBy -->

    <owl:ObjectProperty rdf:about="http://www.essepuntato.it/2008/12/pattern#isContainedBy">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/isPartOf"/>
        <rdfs:comment xml:lang="en">An element that is contained by another structured element.</rdfs:comment>
        <rdfs:label xml:lang="en">is contained by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/oa#hasSelector -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/oa#hasSelector">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/oa#ResourceSelection"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/oa#Selector"/>
        <rdfs:comment>The object of the relationship is a Selector that describes the segment or region of interest within the source resource.  Please note that the domain ( oa:ResourceSelection ) is not used directly in the Web Annotation model.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/oa#"/>
        <rdfs:label>hasSelector</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#generated -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#generated">
        <owl:inverseOf rdf:resource="http://www.w3.org/ns/prov#wasGeneratedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#used -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#used">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#wasInfluencedBy"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Activity"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <rdfs:comment xml:lang="en">A prov:Entity that was used by this prov:Activity. For example, :baking prov:used :spoon, :egg, :oven .</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/prov-o#"/>
        <rdfs:label>used</rdfs:label>
        <prov:category>starting-point</prov:category>
        <prov:component>entities-activities</prov:component>
        <prov:inverse>wasUsedBy</prov:inverse>
        <prov:qualifiedForm rdf:resource="http://www.w3.org/ns/prov#Usage"/>
        <prov:qualifiedForm rdf:resource="http://www.w3.org/ns/prov#qualifiedUsage"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#wasAssociatedWith -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#wasAssociatedWith">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#wasInfluencedBy"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Activity"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <rdfs:comment xml:lang="en">An prov:Agent that had some (unspecified) responsibility for the occurrence of this prov:Activity.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/prov-o#"/>
        <rdfs:label>wasAssociatedWith</rdfs:label>
        <prov:category>starting-point</prov:category>
        <prov:component>agents-responsibility</prov:component>
        <prov:inverse>wasAssociateFor</prov:inverse>
        <prov:qualifiedForm rdf:resource="http://www.w3.org/ns/prov#Association"/>
        <prov:qualifiedForm rdf:resource="http://www.w3.org/ns/prov#qualifiedAssociation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#wasGeneratedBy -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#wasGeneratedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#wasInfluencedBy"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Activity"/>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/prov-o#"/>
        <rdfs:label>wasGeneratedBy</rdfs:label>
        <prov:category>starting-point</prov:category>
        <prov:component>entities-activities</prov:component>
        <prov:inverse>generated</prov:inverse>
        <prov:qualifiedForm rdf:resource="http://www.w3.org/ns/prov#Generation"/>
        <prov:qualifiedForm rdf:resource="http://www.w3.org/ns/prov#qualifiedGeneration"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#wasInfluencedBy -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#wasInfluencedBy"/>
    


